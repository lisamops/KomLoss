journal = {Assistive Technology : The Official Journal of RESNA},
keywords = {assistive technology,caregiver support,cognitive disability,public transit,smartphones,transit apps},
number = {26},
pages = {209--218},
title = {{Transit apps for people with brain injury and other cognitive disabilities: The state of the art}},
url = {http://www.tandfonline.com/loi/uaty20 http://dx.doi.org/10.1080/10400435.2014.930076 http:// www.tandfonline.com/},
volume = {264},
