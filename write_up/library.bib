journal = {Research in Developmental Disabilities},
keywords = {Adolescent,Attention,Attention: physiology,Child,Child Language,Cognition,Cognition: physiology,Executive Function,Executive Function: physiology,Female,Humans,Intellectual Disability,Intellectual Disability: physiopathology,Intellectual Disability: psychology,Male,Memory,Memory: physiology,Models,Neural Inhibition,Neural Inhibition: physiology,Predictive Value of Tests,Psychological,Short-Term,Short-Term: physiology,Verbal Behavior,Verbal Behavior: physiology},
number = {2},
