pages = {294--302},
pmid = {15785265},
title = {{Toward a Taxonomy of Assistive Technology Device Outcomes}},
url = {http://content.wkhealth.com/linkback/openurl?sid=WKPTLP:landingpage{\&}an=00002060-200504000-00011},
volume = {84},
year = {2005}
}
@article{Mortenson2015,
abstract = {INTRODUCTION: Assistive technology is often recommended with the aim of increasing user independence and reducing the burden on informal caregivers. However, until now, there has been no tool to measure the outcomes of this process for caregivers.$\backslash$n$\backslash$nOBJECTIVES: To describe the development of the Caregiver Assistive Technology Outcome Measure (CATOM), a tool developed to measure the impact of assistive technology interventions on the burden experienced by informal caregivers, and to undertake preliminary evaluation of its psychometric properties.$\backslash$n$\backslash$nMETHODS: Based on an existing conceptual framework, existing measures were reviewed to identify potential items in a preliminary version of the measure. Cognitive interviewing was used to identify items needing clarification. A revised CATOM and manual were then reviewed by clinicians. After revising some items based on the interview findings, the measure was piloted as part of an intervention study examining the impact of assistive technology on the users' informal caregivers (n = 44).$\backslash$n$\backslash$nRESULTS: Based on a review of 12 existing measures, a 3-part measure was developed and questions were refined based on cognitive interviews with informal caregivers and feedback experienced assistive technology practitioners. For the activity-specific and overall portions of the measure, the 6-week, test-retest intraclass correlations coefficients were 0.88 (95{\%} CI 0.64-0.96) and 0.86 (95{\%} CI 0.60-0.95), respectively. The CATOM data correlated as hypothesized with other measures.$\backslash$n$\backslash$nCONCLUSION: The CATOM is a promising measure with good content validity and encouraging psychometric properties.},
author = {Mortenson, W. Ben and Demers, Louise and Fuhrer, Marcus J. and Jutai, Jeffrey W. and Lenker, James A. and {De Ruyter}, Frank},
doi = {10.2340/16501977-1952},
file = {:Users/lispa86/Library/Application Support/Mendeley Desktop/Downloaded/Mortenson et al. - 2015 - Development and preliminary evaluation of the caregiver assistive technology outcome measure.pdf:pdf},
isbn = {1651-2081
1650-1977},
issn = {16501977},
journal = {Journal of Rehabilitation Medicine},
keywords = {Assistive technology,Caregiver burden,Caregivers,Informal caregivers,Instrument development,Instrumentation,Self-help devices},
number = {5},
pages = {412--418},
pmid = {25783142},
title = {{Development and preliminary evaluation of the caregiver assistive technology outcome measure}},
url = {http://www.medicaljournals.se/jrm/content/?doi=10.2340/16501977-1952},
volume = {47},
year = {2015}
}
@article{Lenker2010,
abstract = {Recent reports in the rehabilitation literature have suggested that treatment theory, intervention specification, and treatment fidelity have important implications for the design, results, and interpretation of outcomes research. At the same time, there has been relatively little discussion of how these concepts bear on the quality of assistive technology (AT) outcomes research. This article describes treatment theory, intervention specification, and treatment fidelity as interconnected facets of AT outcome studies that fundamentally affect the interpretation of their findings. The discussion of each is elucidated using case examples drawn from the AT outcomes research literature. Recommendations are offered for strengthening these components of AT outcomes research.},
author = {Lenker, James A. and Fuhrer, Marcus J. and Jutai, Jeffrey W. and Demers, Louise and Scherer, Marcia J. and DeRuyter, Frank},
