journal = {Research in Developmental Disabilities},
keywords = {Boxplot,Delta plot,Developmental disabilities,Differential item functioning,Homogeneity of variance,Kurtosis,Matching design,Skewness,developmental disabilities},
number = {6},
pages = {2134--2147},
pmid = {21856117},
publisher = {Elsevier Ltd},
title = {{Beyond matching on the mean in developmental disabilities research}},
