@article{Gathercole2019,
abstract = {We present a new framework characterizing training-induced changes in WM as the acquisition of novel cognitive routines akin to learning a new skill. Predictions were tested in three studies analyzing the transfer between WM tasks following WM training. Study 1 reports a meta-analysis establishing substantial transfer when trained and untrained tasks shared either a serial recall, complex span or backward span paradigm. Transfer was weaker for serial recall of verbal than visuo-spatial material, suggesting that this paradigm is served by an existing verbal STM system and does not require a new routine. Re-analysis of published WM training data in Study 2 showed that transfer was restricted to tasks sharing properties proposed to require new routines. In a re-analysis of data from four studies, Study 3 demonstrated that transfer was greatest for children with higher fluid cognitive abilities. These findings suggest that development of new routines depends on general cognitive resources and that they can only be applied to other similarly-structured tasks.},
author = {Gathercole, Susan E and Dunning, Darren L. and Holmes, Joni and Norris, Dennis},
doi = {10.1016/j.jml.2018.10.003},
issn = {0749596X},
journal = {Journal of Memory and Language},
keywords = {Cognitive routine,Training,Transfer,Working memory},
month = {apr},
pages = {19--42},
publisher = {Academic Press Inc.},
title = {{Working memory training involves learning new skills}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0749596X18300871},
volume = {105},
year = {2019}
}
