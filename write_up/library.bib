file = {:Users/lispa86/Library/Application Support/Mendeley Desktop/Downloaded/Boot et al. - 2018 - Access to assistive technology for people with intellectual disabilities A systematic review to identify barriers a.pdf:pdf},
issn = {13652788},
keywords = {Access,Assistive products,Assistive technology,Intellectual disability,Review},
month = {oct},
number = {10},
pages = {900--921},
title = {{Access to assistive technology for people with intellectual disabilities: A systematic review to identify barriers and facilitators}},
url = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/jir.12532 http://doi.wiley.com/10.1111/jir.12532},
